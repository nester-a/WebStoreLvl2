{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http.HttpClient.WebStoreAPIIdentity": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning",
        "System.Net.Http.HttpClient.WebStoreAPIIdentity": "Warning"
      },
      "FormatterName": "Simple",
      "FormatterOptions": {
        "TimestampFormat": "HH:mm:ss.ffff\t",
        "IncludeScopes": true
      }
    }
  },
  "AllowedHosts": "*",
   "WebAPI": "http://localhost:5001"
}

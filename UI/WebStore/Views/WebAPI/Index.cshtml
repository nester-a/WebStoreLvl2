@model IEnumerable<string>
@{
    ViewBag.Title = "WebAPI Test";
}

<div class="container">
    <table class="table">
        <thead>
            <tr>#</tr>
            <tr>Value</tr>        
        </thead>
        <tbody>
            @{
                var i = 1;
                foreach (var value in Model)
                {
                    <tr>
                        <td>@(i++)</td>
                        <td>@value</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>